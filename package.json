{
  "name": "react-redux-boiler",
  "version": "2.0.3",
  "description": "Boilerplate for react, redux, and typescript",
  "main": "./src/index.tsx",
  "scripts": {
    "start": "webpack-dev-server --config=webpack/webpack.development.config.js --mode=development",
    "build": "webpack --config=webpack/webpack.config.js --mode=production",
    "test": "jest"
  },
  "keywords": [
    "react",
    "redux",
    "boilerplate"
  ],
  "author": "Dylan Paulus",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.1",
    "path": "^0.12.7",
    "prettier": "^1.13.7",
    "react-testing-library": "^4.1.3",
    "ts-jest": "^23.0.1",
    "ts-loader": "^4.4.2",
    "typescript": "^2.9.2",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)+(spec|test).(j|t)s?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      "dist",
      "public",
      "webpack"
    ]
  }
}
